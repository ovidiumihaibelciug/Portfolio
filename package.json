    "google-map-react": "^1.0.5",
    "history": "^4.7.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "progressbar.js": "^1.0.1",
    "randomcolor": "^0.5.3",
    "react": "^16.4.0",
    "react-animate-on-scroll": "^2.1.5",
    "react-circular-multi-progressbar": "^0.6.5",
